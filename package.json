{
  "name": "generate-short-guid",
  "displayName": "Generate short GUID",
  "description": "Generates the first part of a GUID (first 8 chars)",
  "version": "0.0.1",
  "publisher": "miktemk",
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:miktemk.generateShortGuid"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [{
      "command": "miktemk.generateShortGuid",
      "title": "Generate Short Guid"
    }]
  },
  "keybindings": [
    {
      "command": "miktemk.generateShortGuid",
      "key": "ctrl+shift+u",
      "mac": "cmd+shift+u",
      "when": "editorTextFocus"
    }
  ],
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "node-uuid": "^1.4.7"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}